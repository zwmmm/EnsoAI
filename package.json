{
  "name": "enso-ai",
  "version": "0.2.31",
  "description": "EnsoAI - Git Worktree Manager with AI Agent",
  "main": "./out/main/index.js",
  "author": {
    "name": "EnsoAI Team",
    "email": "admin@j3n5en.com"
  },
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@10.26.2",
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "start": "electron-vite preview",
    "postinstall": "electron-builder install-app-deps",
    "build:mac": "pnpm build && electron-builder --mac",
    "build:mac:unsigned": "pnpm build && CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --mac -c.mac.identity=null -c.mac.notarize=false",
    "build:mac:debug": "pnpm build && CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --mac --arm64 --dir  -c.mac.identity=null -c.mac.notarize=false",
    "build:win": "pnpm build && electron-builder --win",
    "build:linux": "pnpm build && electron-builder --linux",
    "generate:themes": "npx tsx scripts/generate-themes.ts",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "prepare": "husky"
  },
  "dependencies": {
    "@agentclientprotocol/sdk": "^0.12.0",
    "@base-ui/react": "^1.0.0",
    "@electron-toolkit/utils": "^4.0.0",
    "@monaco-editor/react": "^4.7.0",
    "@parcel/watcher": "^2.5.1",
    "@pierre/diffs": "^1.0.4",
    "@shikijs/monaco": "^3.20.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.16",
    "@vscode/ripgrep": "^1.17.0",
    "@xterm/addon-fit": "^0.12.0-beta.141",
    "@xterm/addon-search": "^0.17.0-beta.141",
    "@xterm/addon-unicode11": "^0.10.0-beta.141",
    "@xterm/addon-web-links": "^0.13.0-beta.141",
    "@xterm/addon-webgl": "^0.20.0-beta.140",
    "@xterm/xterm": "^6.1.0-beta.141",
    "class-variance-authority": "^0.7.1",
    "cloudflared": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-updater": "^6.6.2",
    "emoji-picker-react": "^4.16.1",
    "framer-motion": "^12.23.26",
    "iconv-lite": "^0.7.1",
    "jschardet": "^3.1.4",
    "lucide-react": "^0.562.0",
    "match-sorter": "^8.2.0",
    "monaco-editor": "^0.55.1",
    "node-pty": "^1.1.0",
    "pidtree": "^0.6.0",
    "pidusage": "^4.0.1",
    "react": "^19.1.4",
    "react-dom": "^19.1.4",
    "react-markdown": "^10.1.0",
    "rehype-raw": "^7.0.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "shell-env": "^4.0.2",
    "shiki": "^3.20.0",
    "simple-git": "^3.30.0",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.4.0",
    "ws": "^8.18.3",
    "zod": "^4.3.5",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/pidusage": "^2.0.5",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "electron-vite": "^5.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "@types/sqlite3": "^3.1.11"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,css}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "pnpm": {
    "overrides": {
      "dmg-builder": "26.4.0",
      "app-builder-lib": "26.4.0",
      "electron-builder-squirrel-windows": "26.4.0"
    }
  }
}
